<Window x:Class="ZadanieNaAvtomatKollegWPFNET1.StudentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZadanieNaAvtomatKollegWPFNET1"
        mc:Ignorable="d"
                Title="Студент" Height="385" Width="400">
	<Grid>
		<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
			<TextBlock Text="Расписание:" FontSize="16" Margin="0,0,0,10"/>
			<ListBox x:Name="RaspisanieListBox" Height="100" Width="300"/>
			<TextBlock Text="Экзамены:" FontSize="16" Margin="0,20,0,10"/>
			<ListBox x:Name="EkzamensListBox" Height="100" Width="300"/>
		</StackPanel>
	</Grid>
</Window>