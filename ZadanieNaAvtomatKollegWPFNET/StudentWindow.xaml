<Window x:Class="ZadanieNaAvtomatKollegWPFNET.StudentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Студент" Height="385" Width="400">
	<Grid>
		<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
			<TextBlock Text="Расписание:" FontSize="16" Margin="0,0,0,10"/>
			<ListBox x:Name="RaspisanieListBox" Height="100" Width="300"/>
			<TextBlock Text="Экзамены:" FontSize="16" Margin="0,20,0,10"/>
			<ListBox x:Name="EkzamensListBox" Height="100" Width="300"/>
		</StackPanel>
	</Grid>
</Window>